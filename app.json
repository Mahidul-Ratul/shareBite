{
  "expo": {
    "name": "shareBite",
    "slug": "shareBite",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/images.jpeg",
    "scheme": "restate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/images.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mahidulratul.shareBite"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/images.jpeg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/images.jpeg",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "enableFullScreenImage_legacy": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Rubik-Bold.ttf",
            "./assets/fonts/Rubik-ExtraBold.ttf",
            "./assets/fonts/Rubik-Medium.ttf",
            "./assets/fonts/Rubik-Light.ttf",
            "./assets/fonts/Rubik-Regular.ttf",
            "./assets/fonts/Rubik-SemiBold.ttf"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "gradleVersion": "7.6",
            "kotlinVersion": "1.7.22",
            "javaVersion": "17"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f307702e-3eba-4a68-82cc-9f0b675a79d7"
      }
    }
  }
}
